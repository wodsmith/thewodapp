{
  "name": "tanstack-start-example-basic-cloudflare",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "preview": "vite preview",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "postinstall": "pnpm run cf-typegen",
    "lint": "biome lint .",
    "format": "biome format --write . --no-errors-on-unmatched .",
    "check": "biome check .",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:migrate:dev": "wrangler d1 migrations apply wodsmith-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply wodsmith-db --remote",
    "db:seed": "bash ./scripts/seed-all.sh",
    "email:dev": "email dev -d src/react-email -p 3001"
  },

{
	"name": "wodsmith-start",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"serve": "vite preview",
		"test": "vitest run",
		"preview": "pnpm run build && vite preview",
		"lint": "biome lint .",
		"format": "biome format --write . --no-errors-on-unmatched .",
		"check": "biome check .",
		"type-check": "tsgo --noEmit",
		"cf-typegen": "wrangler types",
		"db:push": "drizzle-kit push",
		"mastra:dev": "mastra dev --dir src/ai",
		"db:generate": "drizzle-kit generate",
		"db:studio": "drizzle-kit studio",
		"db:migrate:prod": "wrangler d1 migrations apply wodsmith-db --remote",
		"db:migrate:local": "wrangler d1 migrations apply wodsmith-db-dev --local -c .alchemy/local/wrangler.jsonc",
		"db:seed": "bun scripts/seed/index.ts",
		"db:seed:d1": "wrangler d1 execute wodsmith-db-dev --local --file ./scripts/seed.sql -c .alchemy/local/wrangler.jsonc",
		"db:setup-local": "mkdir -p .alchemy/local/src/db && rm -f .alchemy/local/src/db/migrations && ln -sf ../../../../src/db/migrations .alchemy/local/src/db/migrations && pnpm db:migrate:local",
		"alchemy:configure": "bun alchemy configure",
		"alchemy:dev": "STAGE=dev bun alchemy.run.ts",
		"alchemy:staging": "STAGE=staging bun alchemy.run.ts",
		"deploy": "bun alchemy.run.ts",
		"deploy:staging": "pnpm alchemy:staging",
		"deploy:destroy": "bun alchemy.run.ts --destroy",
		"db:seed:e2e": "pnpm tsx scripts/setup-e2e-db.ts",
		"db:seed:pr": "tsx scripts/seed-pr.ts",
		"e2e": "playwright test",
		"e2e:ui": "playwright test --ui",
		"stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe --forward-connect-to localhost:3000/api/webhooks/stripe",
		"email:dev": "pnpm dlx react-email@latest dev -d src/react-email"
	},

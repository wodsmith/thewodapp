{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "team-memory",
	"main": "src/index.ts",
	"compatibility_date": "2026-02-16",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "team-memory-db",
			"database_id": "placeholder"
		}
	],
	"vectorize": [
		{
			"binding": "VECTORIZE",
			"index_name": "team-memory-embeddings"
		}
	],
	"ai": {
		"binding": "AI"
	}
}

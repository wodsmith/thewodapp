{
	"name": "wodsmith",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"build:prod": "pnpm run opennext:build && wrangler deploy --dry-run --outdir=dist",
		"opennext:build": "opennextjs-cloudflare build",
		"build:analyze": "ANALYZE=true next build",
		"start": "next start",
		"lint": "biome lint .",
		"format": "biome format --write . --no-errors-on-unmatched .",
		"check": "biome check .",
		"type-check": "pnpm exec tsc --noEmit",
		"type-check:changed": "pnpm exec node scripts/type-check-changed.mjs",
		"deploy:prod": "pnpm run opennext:build && opennextjs-cloudflare deploy",
		"preview": "pnpm run opennext:build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"db:generate": "drizzle-kit generate",
		"db:studio": "drizzle-kit studio",
		"db:migrate:dev": "wrangler d1 migrations apply $(node scripts/get-db-name.mjs) --local",
		"db:seed": "bash ./scripts/seed-all.sh",
		"db:migrate:prod": "wrangler d1 migrations apply $(node scripts/get-db-name.mjs) --remote",
		"db:migrate:staging": "wrangler d1 migrations apply wodsmith-db-staging --remote",
		"db:seed:staging": "bash ./scripts/seed-all-staging.sh",
		"d1:cache:clean": "wrangler d1 execute $(node scripts/get-db-name.mjs) --command \"DELETE FROM tags; DELETE FROM revalidations\" --remote",
		"d1:cache:clean:staging": "wrangler d1 execute wodsmith-db-staging --command \"DELETE FROM tags; DELETE FROM revalidations\" --remote",
		"email:dev": "email dev -d src/react-email -p 3001",
		"list:kv": "wrangler kv key list --namespace-id=$(node scripts/get-kv-id.mjs) --remote | jq '[.[].name]' > kv.log",
		"delete:kv": "wrangler kv bulk delete kv.log --namespace-id=$(node scripts/get-kv-id.mjs) --remote",
		"test": "vitest run",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome lint --no-errors-on-unmatched",
			"biome format --write --no-errors-on-unmatched"
		]
	},
	"husky": {
		"hooks": {
			"pre-push": "pnpm type-check"
		}
	},
	"dependencies": {
		"@atlaskit/pragmatic-drag-and-drop": "^1.7.4",
